{
  "name": "esy-libuiohook",
  "version": "1.0.0",
  "description": "Mirror of libuiohook",
  "license": "MIT",
  "esy": {
    "build": [
        ["./bootstrap.sh"],
        ["./configure"],
        "make"
    ],
    "buildsInSource": "src/.lib",
    "exportedEnv": {
        "LIBUIOHOOK_INCLUDE_PATH": {
            "val": "#{self.install / 'include' / 'libuiohook'}",
            "scope": "global"
        },
        "LIBUIOHOOK_LIB_PATH": {
            "val": "#{self.lib}",
            "scope": "global"
        }
    }
  },
  "dependencies": {
      "esy-cmake": "^0.3.2"
  },
  "devDependencies": {
      "esy-cmake": "^0.3.2"
  }
}